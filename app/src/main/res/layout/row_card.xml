<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/row_card_cl_container"
    android:layout_width="match_parent"
    android:layout_height="500dp"
    android:layout_margin="15dp"
    android:background="#FFFFFF"
    android:elevation="6dp">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:elevation="6dp"
        android:text="Ratings"
        android:textAlignment="textStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_21"
        app:layout_constraintEnd_toStartOf="@+id/card_star_21"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/card_star_21" />

    <ImageView
        android:id="@+id/card_star_11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_12"
        app:layout_constraintEnd_toStartOf="@+id/card_star_12"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        app:layout_constraintTop_toTopOf="@+id/card_star_12" />

    <ImageView
        android:id="@+id/card_star_35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="20dp"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_34"
        app:layout_constraintTop_toBottomOf="@+id/card_star_25" />

    <ImageView
        android:id="@+id/card_star_12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_13"
        app:layout_constraintEnd_toStartOf="@+id/card_star_13"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_11"
        app:layout_constraintTop_toTopOf="@+id/card_star_13" />

    <ImageView
        android:id="@+id/card_star_14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_15"
        app:layout_constraintEnd_toStartOf="@+id/card_star_15"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_13"
        app:layout_constraintTop_toTopOf="@+id/card_star_15" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:elevation="6dp"
        android:text="Social Presence"
        android:textAlignment="textStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_11"
        app:layout_constraintEnd_toStartOf="@+id/card_star_11"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/card_star_11" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="98dp"
        android:layout_height="24dp"
        android:layout_marginStart="20dp"
        android:elevation="6dp"
        android:text="Accessibility"
        android:textAlignment="textStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_31"
        app:layout_constraintEnd_toStartOf="@+id/card_star_31"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/card_star_31" />

    <ImageView
        android:id="@+id/card_star_34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_35"
        app:layout_constraintEnd_toStartOf="@+id/card_star_35"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_33"
        app:layout_constraintTop_toTopOf="@+id/card_star_35" />

    <ImageView
        android:id="@+id/card_star_15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="20dp"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_14"
        app:layout_constraintTop_toBottomOf="@+id/card_rating" />

    <ImageView
        android:id="@+id/card_star_33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_34"
        app:layout_constraintEnd_toStartOf="@+id/card_star_34"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_32"
        app:layout_constraintTop_toTopOf="@+id/card_star_34" />

    <ImageView
        android:id="@+id/card_star_25"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="20dp"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_24"
        app:layout_constraintTop_toBottomOf="@+id/card_star_15" />

    <ImageView
        android:id="@+id/card_star_31"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_32"
        app:layout_constraintEnd_toStartOf="@+id/card_star_32"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView8"
        app:layout_constraintTop_toTopOf="@+id/card_star_32" />

    <ImageView
        android:id="@+id/card_star_24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_25"
        app:layout_constraintEnd_toStartOf="@+id/card_star_25"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_23"
        app:layout_constraintTop_toTopOf="@+id/card_star_25" />

    <ImageView
        android:id="@+id/card_star_21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_22"
        app:layout_constraintEnd_toStartOf="@+id/card_star_22"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView7"
        app:layout_constraintTop_toTopOf="@+id/card_star_22" />

    <ImageView
        android:id="@+id/card_star_13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_14"
        app:layout_constraintEnd_toStartOf="@+id/card_star_14"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_12"
        app:layout_constraintTop_toTopOf="@+id/card_star_14" />

    <ImageView
        android:id="@+id/card_star_32"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_33"
        app:layout_constraintEnd_toStartOf="@+id/card_star_33"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_31"
        app:layout_constraintTop_toTopOf="@+id/card_star_33" />

    <ImageView
        android:id="@+id/card_star_23"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_24"
        app:layout_constraintEnd_toStartOf="@+id/card_star_24"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_22"
        app:layout_constraintTop_toTopOf="@+id/card_star_24" />

    <ImageView
        android:id="@+id/card_star_22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:src="@drawable/ic_baseline_star_24"
        app:layout_constraintBottom_toBottomOf="@+id/card_star_23"
        app:layout_constraintEnd_toStartOf="@+id/card_star_23"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_star_21"
        app:layout_constraintTop_toTopOf="@+id/card_star_23" />

    <TextView
        android:id="@+id/card_company_name"
        android:layout_width="307dp"
        android:layout_height="49dp"
        android:layout_marginTop="15dp"
        android:elevation="6dp"
        android:text="A company"
        android:textAlignment="center"
        android:textColor="#000000"
        android:textSize="30sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/card_rating"
        android:layout_width="307dp"
        android:layout_height="wrap_content"
        android:elevation="6dp"
        android:text="0.0/10.0"
        android:textAlignment="center"
        android:textColor="#555555"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card_company_name" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/card_btn_like"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:elevation="6dp"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/card_btn_dislike"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/ic_baseline_thumb_up_24" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/card_btn_dislike"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:clickable="true"
        android:elevation="6dp"
        app:backgroundTint="#FFFFFF"
        app:elevation="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/card_btn_like"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:srcCompat="@drawable/ic_baseline_thumb_down_24" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="200dp"
        android:layout_height="1dp"
        android:layout_marginTop="75dp"
        android:background="#000000"
        android:elevation="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/card_star_35"
        app:srcCompat="@android:drawable/divider_horizontal_textfield" />


